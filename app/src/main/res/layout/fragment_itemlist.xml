<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/itemlist_layout">

    <TextView
        android:id="@+id/itemlist_name"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:background="#224367"
        android:text="Navn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        />

    <TextView
        android:id="@+id/itemlist_price"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:background="#824367"
        android:text="Pris"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/itemlist_name"
        />
    <TextView
        android:id="@+id/itemlist_desc"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:background="#424367"
        android:text="Beskrivelse"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/itemlist_price"
        />
    
</androidx.constraintlayout.widget.ConstraintLayout>